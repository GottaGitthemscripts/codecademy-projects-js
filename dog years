// Sets my age
const myAge = 15;
// Early Years
let earlyYears = 2;
earlyYears *= 10.5;
// Later Years
let laterYears = myAge - 2;
// Multiply by 4 and assign later years
laterYears *= 4;
// Return age in dog years
var myAgeInDogYears = earlyYears + laterYears;
// my name in lowercase
const myName = "Jacob".toLowerCase();
// The final product
console.log(
  `My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`
);
